{
    "paths": {
        "home": {
            "template": "page"
        },
        "blog-index": {
            "template": "blog-list"
        }
    },
    "patterns": {
        "blog": {
            "pattern": "blog\/(\\d{4})\/(\\d{2})\/(\\d{2})\/(.+)",
            "template": "blog-post"
        },
        "blog-category": {
            "pattern": "blog\/category\/(.+)",
            "template": "blog-list"
        },
        "blog-archive": {
            "pattern": "blog\/(\\d{4})\/(\\d{2})",
            "template": "blog-list"
        }
    }
}